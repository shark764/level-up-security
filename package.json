{
  "name": "level-up-security",
  "version": "1.0.0",
  "engines": {
    "node": ">=14"
  },
  "description": "Level Up - Security Module Administration",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.+(js|ts|json)\"",
    "dev": "nodemon src/index.js",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "eslint \"**/*.{js,ts}\" --fix",
    "pretest": "eslint --ignore-path .gitignore .",
    "prettify": "npm run format && npm run lint:fix",
    "updateModels": "git submodule update --remote",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "release": "release-it"
  },
  "author": "Gerardo Zarate, Fernando Rivas",
  "license": "ISC",
  "dependencies": {
    "@level-up/utilities": "^0.1.3",
    "await-to-js": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "ioredis": "^4.22.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.18",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google": "^0.3.0",
    "passport-google-oauth20": "^2.0.0",
    "rand-token": "^1.0.1",
    "randomatic": "^3.1.1",
    "redis": "^3.0.2",
    "validator": "^13.5.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@commitlint/config-conventional": "^13.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "release-it": "^14.11.5"
